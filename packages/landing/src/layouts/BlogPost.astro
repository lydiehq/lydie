---
import Layout from "./Layout.astro";
import { Container } from "../components/Container";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Layout title={title} description={""}>
  <div class="py-16">
    <Container>
      <div class="mx-auto">
        <slot />
      </div>
    </Container>
  </div>
</Layout>
